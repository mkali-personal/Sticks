  {
    "name": "TabNameEditor",
    "version": "1.0",
    "description": "Changes the title of the tab!",
    "background": {
	"scripts": ["background.js"],
	"persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "images/edit-128.png"
      }
    },
    "icons": {
      "128": "images/edit-128.png"
    },
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "options_page": "options.html",
    "commands": {
          "_execute_page_action": {
            "suggested_key": {
              "default": "Ctrl+Shift+Z",
              "windows": "Ctrl+Shift+Z",
              "mac": "Alt+Shift+P"
            }
          }
        },
    "manifest_version": 2
  }